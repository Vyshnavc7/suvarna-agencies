<section class="banner-bottom-wthreelayouts py-lg-5 py-3">
	<div class="container-fluid">
		<div class="inner-sec-shop px-lg-4 px-3">
			<hr>
			<h3 class="tittle-w3layouts my-lg-4 my-4">New Arrivals of machines </h3>

			<!-- 1st 4 machines starts -->
			
			<div class="row">
				<!-- /makitha drill -->
				{{#each products}}
				<div class="col-md-3 product-men women_two propad" style="padding-right: 21px;padding-left: 21px;">
					<div class="product-googles-info googles propad">
						<div class="men-pro-item">
							<div class="men-thumb-item">
								<img src="/product-images/{{this._id}}.jpg" class="img-fluid" alt="">
								<div class="men-cart-pro">
									<div class="inner-men-cart-pro">
										<!-- single.html page down -->
										<a href="404.html" class="link-product-add-cart">Quick View</a>
									</div>
								</div>
								<span class="product-new-top">New</span>
							</div>
							<div class="item-info-product">
								<div class="info-product-price">
									<div class="grid_meta">
										<div class="product_price">
											<h4>
												<!-- single.html page down-->
												<a href="404.html">{{this.name}} </a>
												<p>{{this.det}}</p>
											</h4>
											<div class="grid-price mt-2">
												<i class="fa fa-rupee rupecolor"></i>
												<span class="money ">{{this.price}}</span>
											</div>
										</div>

									</div>
									<div class="googles single-item hvr-outline-out">
										<form action="#" method="post">
											<input type="hidden" name="cmd" value="_cart">
											<input type="hidden" name="add" value="1">
											<input type="hidden" name="googles_item" value="Farenheit">
											<input type="hidden" name="amount" value="{{this.price}}">
											<button type="submit" class="googles-cart pgoogles-cart">
												<i class="fas fa-cart-plus"></i>
											</button>


										</form>

									</div>
								</div>
								<div class="clearfix"></div>
							</div>
						</div>
					</div>
				</div>
			{{/each}}

			</div>
			

			
		<hr>
		<!-- /grids -->
		<div class="bottom-sub-grid-content py-lg-5 py-3">
			<div class="row">
